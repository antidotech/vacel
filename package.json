{
  "name": "vaceline",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": "git@github.com:shqld/vaceline.git",
  "author": "Sho Miyamoto <me@shomiyamoto.dev>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "vaceline": "./dist/cli/index.js"
  },
  "scripts": {
    "start": "npm run -s build > /dev/null && node dist/cli",
    "build": "babel src -d dist -x .ts,.js --delete-dir-on-start",
    "inspect": "yarn lint && yarn typecheck && yarn test",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "bench": "node benchmark > bench.txt",
    "build-doc": "node tasks/build-doc",
    "coverage": "jest --coverage",
    "dev:web": "rimraf web/dist && cp -r web/src/public web/dist && yarn webpack --watch --dev --config web/webpack.config.ts",
    "build:web": "rimraf web/dist && cp -r web/src/public web/dist && yarn webpack --config web/webpack.config.ts",
    "publish:web": "yarn build:web && yarn gh-pages -d web/dist"
  },
  "dependencies": {
    "@textlint/markdown-to-ast": "^6.1.6",
    "chalk": "^2.4.2",
    "change-case": "^4.1.1",
    "debug": "^4.1.1",
    "is-ip": "^3.1.0",
    "mkdirp": "^0.5.1",
    "prettier": "^2.2.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^8.2.0",
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.0.8",
    "@types/prettier": "^1.18.3",
    "@types/rimraf": "^2.0.3",
    "@types/webpack": "^4.41.1",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^1.7.0",
    "@types/webpackbar": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-loader": "^8.0.6",
    "benchmark": "^2.1.4",
    "comlink": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^3.1.0",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "jest-snapshot-serializer-raw": "^1.1.0",
    "lerna": "^3.22.1",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "preact": "^10.1.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "serve": "^11.3.0",
    "source-map-support": "^0.5.19",
    "strip-ansi": "^6.0.0",
    "ts-morph": "^6.0.2",
    "typescript": "^4.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "webpackbar": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn pretty-quick && yarn inspect"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run -s typecheck",
      "npm run -s lint"
    ]
  }
}
